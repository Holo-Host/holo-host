{
  "name": "@holo/cloud-console",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "exports": "./vite/build.ts",
  "compilerOptions": {
    "checkJs": true,
    "strict": true,
    "rootDirs": [".", "./.svelte-kit", "./.svelte-kit/types"],
    "lib": [
      "deno.window",
      "deno.ns",
      "ESNext",
      "DOM",
      "DOM.Iterable",
      "DOM.AsyncIterable",
      "webworker"
    ]
  },
  "tasks": {
    "dev": "deno run -A vite/dev.ts",
    "build": "deno run -A vite/build.ts"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
    "@sveltejs/adapter-static": "npm:@sveltejs/adapter-static@^3.0.8",
    "@sveltejs/kit": "npm:@sveltejs/kit@^2.21.0",
    "@sveltejs/vite-plugin-svelte": "npm:@sveltejs/vite-plugin-svelte@^5.0.3",
    "esm-env": "npm:esm-env@^1.2.2",
    "svelte": "npm:svelte@^5.28.6",
    "vite": "npm:vite@^6.3.5"
  }
}

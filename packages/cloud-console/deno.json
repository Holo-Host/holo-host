{
  "name": "@holo/cloud-console",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "exports": "./src/app.html",
  "compilerOptions": {
    "checkJs": true,
    "strict": true,
    "rootDirs": [".", "./.svelte-kit"],
    "lib": [
      "deno.window",
      "deno.ns",
      "ESNext",
      "DOM",
      "DOM.Iterable",
      "DOM.AsyncIterable"
    ]
  },
  "tasks": {
    "dev": "deno run -A --node-modules-dir=auto vite/dev.ts",
    "build": "deno run -A --node-modules-dir=auto vite/build.ts"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
    "@sveltejs/vite-plugin-svelte": "npm:@sveltejs/vite-plugin-svelte@^5.0.3",
    "svelte": "npm:svelte@^5.28.6",
    "vite": "npm:vite@^6.3.5"
  }
}
